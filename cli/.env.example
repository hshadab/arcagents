# Arc Agent CLI Configuration
# Copy this file to .env for local development

# Arc Network RPC
ARC_TESTNET_RPC=https://rpc.testnet.arc.network

# Default wallet private key for CLI operations
# WARNING: Never commit real private keys to version control
CLI_PRIVATE_KEY=your_private_key_here

# Circle API (for wallet and compliance features)
CIRCLE_API_KEY=your_circle_api_key_here

# JOLT-Atlas zkML Prover Service
JOLT_ATLAS_SERVICE_URL=http://localhost:3001

# Contract Addresses (Arc Testnet)
ARC_AGENT_ADDRESS=0x982Cd9663EBce3eB8Ab7eF511a6249621C79E384
ARC_PROOF_ATTESTATION_ADDRESS=0xBE9a5DF7C551324CB872584C6E5bF56799787952
