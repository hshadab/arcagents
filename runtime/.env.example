# Agent Runtime Configuration
# Copy this file to .env and fill in the values

# Agent configurations as JSON array
# See src/cli.ts for example config structure
AGENT_CONFIGS=[]

# Agent private keys (referenced by privateKeyEnvVar in config)
# Each agent should have its own key environment variable
# Example: AGENT_MY_AGENT_KEY=0x...
# WARNING: Never commit real private keys to version control

# Network Configuration
# Supported networks: base, base-sepolia
DEFAULT_NETWORK=base

# JOLT-Atlas zkML Prover Service
JOLT_ATLAS_SERVICE_URL=http://localhost:3001

# Arc Network RPC (for proof attestation)
ARC_TESTNET_RPC=https://rpc.testnet.arc.network

# Contract Addresses (Arc Testnet)
ARC_PROOF_ATTESTATION_ADDRESS=0xBE9a5DF7C551324CB872584C6E5bF56799787952

# Optional: Webhook URL for execution notifications
WEBHOOK_URL=
